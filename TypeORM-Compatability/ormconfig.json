{
   "type": "mysql",
   "host": "<HOSTNAME>",
   "username": "<USERNAME>",
   "password": "<PASSWORD>",
   "database": "<DATABASE_NAME>",
   "synchronize": true,
   "logging": false,
   "entities": [
      "src/entity/**/*.ts"
   ],
   "migrations": [
      "src/migration/**/*.ts"
   ],
   "subscribers": [
      "src/subscriber/**/*.ts"
   ],
   "cli": {
      "entitiesDir": "src/entity",
      "migrationsDir": "src/migration",
      "subscribersDir": "src/subscriber"
   }
}